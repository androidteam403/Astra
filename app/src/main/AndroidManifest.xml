<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.thresholdsoft.astra">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round_apollo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_apollo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Astra">
        <activity
            android:name=".RequestHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.picklisthistory.PickListHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.userlogin.UserLoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.thresholdsoft.astra.ui.main.AstraMainActivity"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thresholdsoft.astra.ui.home.Home"
            android:screenOrientation="landscape" />
    </application>

</manifest>